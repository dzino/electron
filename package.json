{
  "name": "alarm-threshold-graph",
  "main": "public/electron.js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.5.2",
    "dateformat": "^4.5.1",
    "react": "^17.0.2",
    "react-bootstrap": "1.3.0",
    "react-circular-input": "0.2.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "testDev": "yarn jest",
    "testProd": "yarn jest -production",
    "eject": "react-scripts eject",
    "electron": "wait-on tcp:3000 && electron .",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "web": "react-scripts build",
    "linux": "react-scripts build && sed -i 's/\\/static/\\.\\/static/g' ./build/index.html && electron-packager . alarm-threshold-graph --overwrite --asar=true --platform=linux --arch=x64 --icon=public/icons/512x512.png --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^6.3.0",
    "electron": "^15.1.0",
    "electron-is-dev": "^2.0.0",
    "jest": "^27.1.1",
    "ts-jest": "^27.0.5",
    "wait-on": "^6.0.0"
  }
}
